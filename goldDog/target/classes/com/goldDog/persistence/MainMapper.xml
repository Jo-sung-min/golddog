<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goldDog.persistence.MainMapper">

	<select id="getMember" resultType="com.goldDog.domain.MemberVO">
		select * from member
	</select>
	
	<select id="getTrainer" resultType="com.goldDog.domain.TrainerVO">
		select * from trainer where m_no=#{m_no}
	</select>


	<!-- 최신 평점순으로 5개만 가져오는걸로 수정해야함 -->
	<select id="getReview" resultType="com.goldDog.domain.ReviewVO">
		select * from Review 
	</select>





</mapper>